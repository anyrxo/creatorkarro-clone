{
  "name": "nextjs-shadcn",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"npm run start-api\" \"next dev -H 0.0.0.0 --turbopack\"",
    "dev-live": "concurrently --kill-others \"npm run start-api\" \"NEXT_CONFIG_PATH=next.config.dev.js next dev -H 0.0.0.0\"",
    "dev-frontend-only": "next dev -H 0.0.0.0 --turbopack",
    "start-api": "cd src/app/fx-calc/api && python3 market_data_server.py",
    "start-api-production": "cd src/app/fx-calc/api && gunicorn --bind 0.0.0.0:5001 --workers 4 --timeout 60 market_data_server:app",
    "start-live": "NEXT_CONFIG_PATH=next.config.dev.js next start -H 0.0.0.0",
    "build-live": "NEXT_CONFIG_PATH=next.config.dev.js next build",
    "production": "./production-startup.sh",
    "build": "next build",
    "build-original": "node scripts/silent-build.js",
    "build-bash": "./build.sh",
    "build-smart": "node scripts/build-with-ignore.js",
    "start": "next start",
    "lint": "npx tsc --noEmit && next lint",
    "format": "npx biome format --write",
    "toxic-seo-expand": "node src/scripts/toxicSeoExpansion.js",
    "generate-massive-blogs": "node src/scripts/massiveBlogGeneration.js",
    "seo-domination": "npm run toxic-seo-expand && npm run generate-massive-blogs",
    "blog-empire-stats": "echo \"Blog Empire: 550 posts across 5 courses\"",
    "generate-blogs": "node src/scripts/simpleBlogGenerator.js",
    "expand-blogs": "node src/scripts/expandBlogGeneration.js",
    "generate-all-blogs": "npm run generate-blogs && npm run expand-blogs",
    "generate-programmatic": "npx ts-node src/scripts/generateProgrammaticPages.ts",
    "validate-meta-descriptions": "node scripts/validate-meta-descriptions.js",
    "fix-meta-descriptions": "node scripts/fix-meta-descriptions.js",
    "optimize-images": "node scripts/optimize-images.js",
    "seo:health": "node scripts/seo-health-check.js",
    "seo:guard": "node scripts/seo-guard.js",
    "seo:schema": "node scripts/validate-all-schema.js",
    "seo:links": "node scripts/analyze-internal-links.js",
    "seo:sitemap": "node scripts/generate-sitemap.js",
    "seo:performance": "node scripts/monitor-seo-performance.js",
    "seo:report": "node scripts/generate-seo-report.js",
    "seo:all": "npm run seo:health && npm run seo:schema && npm run seo:links && npm run seo:performance && npm run seo:report",
    "seo:install-hook": "node scripts/seo-guard.js --install"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.35.2",
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.5",
    "@supabase/supabase-js": "^2.83.0",
    "@tailwindcss/typography": "^0.5.16",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.475.0",
    "next": "^15.5.6",
    "openai": "^5.11.0",
    "react": "^18.3.1",
    "react-confetti": "^6.4.0",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-use": "^17.6.0",
    "remark-gfm": "^4.0.1",
    "resend": "^6.5.2",
    "sharp": "^0.34.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^20.19.13",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "concurrently": "^8.2.2",
    "eslint": "^9.27.0",
    "eslint-config-next": "^15.5.6",
    "glob": "^11.0.3",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
